{
  "name": "emotion-to-vanilla-extract",
  "packageManager": "yarn@3.3.1",
  "description": "Convert an Emotion CSSObject to a vanilla-extract ComplexStyleRule",
  "version": "0.1.1",
  "module": "index.js",
  "main": "index.js",
  "types": "index.d.ts",
  "devDependencies": {
    "@emotion/serialize": "^1.1.1",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@vanilla-extract/css": "^1.9.5",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.4.2",
    "prettier": "2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "*"
  },
  "scripts": {
